<%- include("partials/header"); %>

<h1>Home</h1>
<p><%= homeStartingContent %></p>

<div>
  <% postItems.forEach(function(postItem){ %>
  <div>
    <h3><%= postItem.title %></h3>
    <p>
      <%= postItem.content.substring(0,100) + " ... " %>
      <a href="/posts/<%= postItem.title %> ">Read More</a>
    </p>
  </div>
  <% }); %>
</div>

<%- include("partials/footer"); %>
